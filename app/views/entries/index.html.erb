<p id="notice"><%= notice %></p>

<h1>Listing Entries</h1>

<%= link_to 'New Entry', new_entry_path %>

<div id="form-container">
  <%= render partial: 'form', locals: {entry: Entry.new} %>
</div>

<table id="entries">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Starts at</th>
      <th>Ends at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @entries %>
  </tbody>
</table>

<br>